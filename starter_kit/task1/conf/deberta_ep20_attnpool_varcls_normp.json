    "loss_type": "mse",
    "bin_size": 0.25,
    "use_cls_head": true,
    "disable_inner_dropout": true,
    "lambda_reg": 1.0,
    "lambda_cls": 1.0,
    "ensemble_alpha": 0.5,
    "score_min": 1.0,
    "score_max": 9.0,
    "pred_head": "normp"
  }
}
